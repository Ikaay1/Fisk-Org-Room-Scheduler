generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

model Rooms {
  id         Int      @id @default(autoincrement())
  building   String
  features   String[]
  roomNumber String?
  capacity   Int
}
